6. 
#include <stdio.h>

int main(int argc, char **argv){
	
	printf("Ola %s\n", argv[1]);
	printf("Numero de Entradas = %d\n", argc);

	return 0;
}

7.
#include <stdio.h>

int main(int argc, char **argv){
	
	int i = 1;

	printf("Argumentos: ");
	while(i!=argc){
		printf("%s ", argv[i]);
		i++;;
	}
	printf("\n");

	return 0;
}

8.
#include <stdio.h>
#include "num_caracs.h"

int main(int argc, char **argv){
	
	printf("%d\n",num_caracs(argv[1]));

	return 0;
}

9.
#include <stdio.h>
#include "num_caracs.h"

int main(int argc, char **argv){

	int i = 0;

	while(i!=argc){
		printf("Argumento: %s // Numero de caracteres: %d\n",argv[i],num_caracs(argv[i]));
		i++;
	}
	
	
	return 0;
}

